  <div class="row">
	 <div class="modal fade login" id="loginModal">
	      <div class="modal-dialog login animated">
  		      <div class="modal-content">
  		         <div class="modal-header">
                      <h4 class="modal-title">로그인</h4>
                      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                  </div>
                  <div class="modal-body">  
                  
                  <%= form_for(:user, :url => session_path(:user)) do |f| %>
                      <div class="box">
                           <div class="content">
                              <div class="error"></div>
                              <div class="form loginBox">
                                  <!--<form method="post" action="/login" accept-charset="UTF-8">-->
                                  <!--<input id="email" class="form-control" type="text" placeholder="이메일을 입력하세요" name="email">-->
                                  <%= f.email_field :email, :class =>"form-control",placeholder: "이메일을 입력하세요", value:"" %>
                                  <!--<input id="password" class="form-control" type="password" placeholder="비밀번호를 입력하세요" name="password">-->
                                  <%= f.password_field :password,:label =>"Password", :class =>"form-control", placeholder:"비밀번호를 입력하세요", value:""%>
                                  <!--<input class="btn btn-default btn-login" type="button" value="로그인" onclick="loginAjax()">-->
                                  <%= f.submit '로그인', :class =>"btn btn-default btn-login" %>
                                  <!--</form>-->
                              </div>
                           </div>
                      </div>
                 <% end %>
                      
                      
                      
                <%= form_for(:user, :url => registration_path(:user)) do |f| %>
                      <div class="box">
                          <div class="content registerBox" style="display:none;">
                           <!--<div class="form">-->
                              <!--<form method="post" html="{:multipart=>true}" data-remote="true" action="/register" accept-charset="UTF-8">-->
                              <!--<input id="email" class="form-control" type="text" placeholder="이메일을 입력하세요." name="email">-->
                              <%= f.email_field :email, :class =>"form-control", placeholder: "이메일을 입력하세요.", :stlye => "margin-bottom:5px" %>
                              <!--<input id="password" class="form-control" type="password" placeholder="비밀번호를 입력하세요." name="password">-->
                              <%= f.password_field :password, :class =>"form-control", placeholder: "비밀번호를 입력하세요." %>
                              <!--<input id="password_confirmation" class="form-control" type="password" placeholder="비밀번호를 확인하세요." name="password_confirmation">-->
