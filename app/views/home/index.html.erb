                              <!--<input class="btn btn-default btn-register" type="submit" value="회원가입" name="commit">-->
                              <%= f.submit "회원가입", :class =>"btn btn-default btn-login" %>
                              <!--</form>-->
                              <!--</div>-->
                          </div>
                      </div>
                 <% end %>
                      
                      
                      
                      
                  </div>
                  
                  
                  
                  
                  
                  
                  
                  
